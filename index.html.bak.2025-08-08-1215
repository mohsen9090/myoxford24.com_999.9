// Remove all privacy popups and overlays
function removePrivacyPopups() { const selectors 
  = [
    '.onetrust-pc-dark-filter', 
    '.onetrust-pc-container', 
    '#onetrust-consent-sdk', '.ot-sdk-container', 
    '[id*="onetrust"]', '[class*="onetrust"]', 
    '[id*="privacy"]', '[class*="privacy"]', 
    '.cookie-banner', '.consent-banner', 
    '.gdpr-banner', 'div[style*="position: 
    absolute"]', 'div[style*="position: fixed"]'
  ];
  
  selectors.forEach(selector => { const elements 
    = document.querySelectorAll(selector); 
    elements.forEach(el => {
      if (el.innerText && ( 
        el.innerText.includes('privacy') || 
        el.innerText.includes('Privacy') || 
        el.innerText.includes('cookie') || 
        el.innerText.includes('Cookie') || 
        el.innerText.includes('consent') || 
        el.innerText.includes('Consent') || 
        el.innerText.includes('onetrust') || 
        el.innerText.includes('OneTrust')
      )) { el.style.display = 'none'; 
        el.style.visibility = 'hidden'; 
        el.style.opacity = '0'; 
        el.style.pointerEvents = 'none'; 
        el.style.zIndex = '-1'; el.remove();
      }
    });
  });
}
// Remove popups every 1 second
setInterval(removePrivacyPopups, 1000);
// Initial cleanup
document.addEventListener('DOMContentLoaded', 
removePrivacyPopups);
// Enhanced Packs Renderer
for(const card of 
document.querySelectorAll('.video-card')){
  const key = card.getAttribute('data-key'); 
  const pack = VIDEO_PACKS[key]; if(!pack) 
  continue; const details = 
  document.createElement('details'); 
  details.className = 'pack'; details.innerHTML = 
  `
    <summary>üó£Ô∏è ${pack.title} <span 
    class="badge">${pack.level}</span></summary> 
    <div class="pack-inner">
      ${pack.items.map(([en,fa])=>( `<div 
        class="line"><div 
        class="en">${en}</div><div 
        class="fa">${fa}</div></div>`
      )).join('')} <div class="mini"> <b>üé≠ 
        Professional Dialogue Example:</b> <div 
        class="line"><div class="en">A: 
        ${pack.mini[0][1]}</div></div> <div 
        class="line"><div class="en">B: 
        ${pack.mini[1][1]}</div><div 
        class="fa">${pack.mini[2][1]}</div></div>
      </div> </div> `; card.appendChild(details);
}
// Smooth scrolling for topic tags
document.querySelectorAll('.topic-tag').forEach(tag 
=> {
  tag.addEventListener('click', function() { 
    const topics = 
    document.querySelectorAll('.section-title'); 
    const randomTopic = 
    topics[Math.floor(Math.random() * 
    topics.length)]; randomTopic.scrollIntoView({ 
    behavior: 'smooth' });
  });
});
// Iframe loading animation
document.querySelectorAll('iframe').forEach(iframe 
=> {
  iframe.addEventListener('load', function() { 
    this.style.opacity = '1'; 
    removePrivacyPopups(); // Remove popups after 
    iframe loads
  });
  iframe.style.opacity = '0.7';
});
// CSS injection to hide popups
const style = document.createElement('style'); 
style.textContent = `
  .onetrust-pc-dark-filter, 
  .onetrust-pc-container,
  #onetrust-consent-sdk,
  .ot-sdk-container, [id*="onetrust"], 
  [class*="onetrust"], [id*="privacy"], 
  [class*="privacy"], .cookie-banner, 
  .consent-banner, .gdpr-banner {
    display: none !important; visibility: hidden 
    !important;
    opacity: 0 !important; pointer-events: none 
    !important;
    z-index: -9999 !important;
  }
`; document.head.appendChild(style); </script> 
</body>
</html>
